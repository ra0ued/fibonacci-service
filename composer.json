{
    "name": "ra0ued/fibonacci-service",
    "description": "Fibonacci Service which can help to get a job at Lesta Games",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7",
        "fibonacci"
    ],
    "homepage": "https://github.com/ra0ued/fibonacci-service",
    "license": "MIT",
    "authors": [
        {
            "name": "test",
            "email": "ra0ued@zabtech.ru",
            "homepage": "https://ra0ued.ru"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-gmp": "*",
        "monolog/monolog": "^3.7",
        "php-di/php-di": "^6.4",
        "phpmath/biginteger": "^1.0",
        "slim/psr7": "^1.7",
        "slim/slim": "^4.14",
        "symfony/cache": "^7.1"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^9.5.26",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
